
@{
    ViewData["Title"] = "Importa fisier";
}

<h2>ImportFile</h2>
<div class="alert alert-danger col-lg-4">@ViewBag.Mesaj</div>
<div class="row">
    <form method="post" enctype="multipart/form-data" asp-controller="home" asp-action="ImportFile">
        <div class="form-group">
            <div class="col-md-10">
                <p>Incarca fisierul cu lista de productie:</p>
                <input type="file" name="files" multiple />
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-10">
                <input type="submit" value="Upload" />
            </div>
        </div>
    </form>
</div>